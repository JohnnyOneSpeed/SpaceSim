    <TargetOrientation>-167</TargetOrientation>
  </Command>
  <Command xsi:type="RelativePitch">
    <StartTime>520</StartTime>
    <Duration>30</Duration>
    <TargetOrientation>-180</TargetOrientation>
  </Command>
  <Command xsi:type="Prograde">
    <StartTime>550</StartTime>
    <Duration>10000</Duration>
    <InitialAdjustmentTime>10</InitialAdjustmentTime>
  </Command>
  <Command xsi:type="Post">
    <StartTime>630</StartTime>
    <Duration>5</Duration>
    <Message>The result is remarkably similar trajectories to the ISS.</Message>
  </Command>
  <Command xsi:type="Zoom">
