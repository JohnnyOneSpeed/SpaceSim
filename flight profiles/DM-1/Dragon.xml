<?xml version="1.0"?>
<ArrayOfCommand xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Command xsi:type="Release">
    <StartTime>0</StartTime>
  </Command>
  <Command xsi:type="RelativePitch">
    <StartTime>10</StartTime>
    <Duration>40</Duration>
    <TargetOrientation>-94</TargetOrientation>
  </Command>
  <Command xsi:type="Post">
    <StartTime>6</StartTime>
    <Duration>5</Duration>
    <Message>Although CRS-16 on the left tried and failed to return its first stage to land ...</Message>
  </Command>
  <Command xsi:type="Post">
    <StartTime>12</StartTime>
    <Duration>5</Duration>
    <Message>... the DM-1 first stage landed on a drone ship, saving propellant.</Message>
  </Command>
  <Command xsi:type="Post">
    <StartTime>18</StartTime>
    <Duration>5</Duration>
    <Message>Some propellant was used to throw the heavier Crew Dragon to orbit, but what about the rest?</Message>
  </Command>
  <Command xsi:type="Zoom">
    <StartTime>18</StartTime>
    <TargetScale>40</TargetScale>
  </Command>
  <Command xsi:type="Post">
    <StartTime>24</StartTime>
    <Duration>5</Duration>
    <Message>The DM-1 trajectory on the right is more lofted ...</Message>
  </Command>
  <Command xsi:type="Rate">
    <StartTime>24</StartTime>
    <TargetIndex>5</TargetIndex>
  </Command>
  <Command xsi:type="Post">
    <StartTime>42</StartTime>
    <Duration>5</Duration>
    <Message>... and it throttles back much further for Max-Q.</Message>
  </Command>
  <Command xsi:type="Zoom">
    <StartTime>42</StartTime>
    <TargetScale>65</TargetScale>
  </Command>
  <Command xsi:type="RelativePitch">
    <StartTime>50</StartTime>
    <Duration>60</Duration>
    <TargetOrientation>-115</TargetOrientation>
  </Command>
  <Command xsi:type="Zoom">
    <StartTime>55</StartTime>
    <TargetScale>90</TargetScale>
  </Command>
  <Command xsi:type="Post">
    <StartTime>60</StartTime>
    <Duration>5</Duration>
    <Message>Peak pressure for CRS-16 was about 24kPa, for DM-1 it was 21kPa.</Message>
  </Command>
  <Command xsi:type="RelativePitch">
    <StartTime>110</StartTime>
    <Duration>45</Duration>
    <TargetOrientation>-148</TargetOrientation>
  </Command>
  <!--<Command xsi:type="Target">
    <StartTime>70</StartTime>
    <Next>true</Next>
  </Command>-->
  <Command xsi:type="Zoom">
    <StartTime>70</StartTime>
    <TargetScale>240</TargetScale>
  </Command>
  <Command xsi:type="Post">
    <StartTime>80</StartTime>
    <Duration>5</Duration>
    <Message>Increasing update speed to 4x.</Message>
  </Command>
  <Command xsi:type="Rate">
    <StartTime>80</StartTime>
    <TargetIndex>6</TargetIndex>
  </Command>
  <Command xsi:type="Zoom">
    <StartTime>80</StartTime>
    <TargetScale>60</TargetScale>
  </Command>
  <Command xsi:type="Zoom">
    <StartTime>100</StartTime>
    <TargetScale>60</TargetScale>
  </Command>
  <Command xsi:type="Zoom">
    <StartTime>120</StartTime>
    <TargetScale>60</TargetScale>
  </Command>
  <Command xsi:type="Stage">
    <StartTime>161</StartTime>
  </Command>
  <Command xsi:type="RelativePitch">
    <StartTime>163</StartTime>
    <Duration>10</Duration>
    <TargetOrientation>-158</TargetOrientation>
  </Command>
  <Command xsi:type="RelativePitch">
    <StartTime>170</StartTime>
    <Duration>100</Duration>
    <TargetOrientation>-170</TargetOrientation>
  </Command>
  <Command xsi:type="Post">
    <StartTime>230</StartTime>
    <Duration>5</Duration>
    <Message>Increasing update speed to 8x.</Message>
  </Command>
  <Command xsi:type="Rate">
    <StartTime>230</StartTime>
    <TargetIndex>7</TargetIndex>
  </Command>
  <Command xsi:type="Zoom">
    <StartTime>230</StartTime>
    <TargetScale>60</TargetScale>
  </Command>
  <Command xsi:type="RelativePitch">
    <StartTime>270</StartTime>
    <Duration>100</Duration>
    <TargetOrientation>-177</TargetOrientation>
  </Command>
  <Command xsi:type="RelativePitch">
    <StartTime>370</StartTime>
    <Duration>50</Duration>
    <TargetOrientation>-163</TargetOrientation>
  </Command>
  <Command xsi:type="RelativePitch">
    <StartTime>420</StartTime>
