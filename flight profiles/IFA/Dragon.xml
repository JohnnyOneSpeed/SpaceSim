<?xml version="1.0"?>
<ArrayOfCommand xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Command xsi:type="Zoom">
    <StartTime>-2</StartTime>
    <TargetScale>-25</TargetScale>
  </Command>
  <Command xsi:type="Release">
    <StartTime>0</StartTime>
  </Command>
  <Command xsi:type="Post">
    <StartTime>6</StartTime>
    <Duration>5</Duration>
    <Message>The Crew Dragon in-flight abort (IFA) mission will initially emulate a regular Crew Dragon profile ...</Message>
  </Command>
  <Command xsi:type="RelativePitch">
    <StartTime>10</StartTime>
    <Duration>40</Duration>
    <TargetOrientation>-94</TargetOrientation>
  </Command>
  <Command xsi:type="Post">
    <StartTime>12</StartTime>
    <Duration>5</Duration>
    <Message>... full throttle to about T+49 seconds, 85% throttle until T+70 ...</Message>
  </Command>
  <Command xsi:type="Post">
    <StartTime>18</StartTime>
    <Duration>5</Duration>
    <Message>... then back up to about 96%.</Message>
  </Command>
  <Command xsi:type="Post">
    <StartTime>24</StartTime>
    <Duration>5</Duration>
    <Message>Acceleration will build to around 2.4g at T+90 seconds, well after MaxQ.</Message>
  </Command>
  <Command xsi:type="Post">
    <StartTime>32</StartTime>
    <Duration>5</Duration>
    <Message>At around Mach 2, S1 will shutdown, and Dragon will abort. Increasing update speed to 2x.</Message>
  </Command>
  <Command xsi:type="Rate">
    <StartTime>32</StartTime>
    <TargetIndex>5</TargetIndex>
  </Command>
