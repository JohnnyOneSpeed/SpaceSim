    <StartTime>32</StartTime>
    <Duration>5</Duration>
    <Message>As the rockets approach Max-Q ...</Message>
  </Command>
  <Command xsi:type="Post">
    <StartTime>38</StartTime>
    <Duration>5</Duration>
    <Message>... Iridium 7 throttles back further than Iridium 8 ...</Message>
