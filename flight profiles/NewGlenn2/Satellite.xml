<?xml version="1.0"?>
<ArrayOfCommand xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Command xsi:type="Post">
    <StartTime>4</StartTime>
    <Duration>5</Duration>
    <Message>There is precious little hard information about New Glenn ...</Message>
  </Command>
  <Command xsi:type="RelativePitch">
    <StartTime>10</StartTime>
    <Duration>40</Duration>
    <TargetOrientation>-105</TargetOrientation>
  </Command>
  <Command xsi:type="Post">
    <StartTime>10</StartTime>
    <Duration>5</Duration>
    <Message>... the first stage has 7 x 250mT SL thrust engines ...</Message>
  </Command>
  <Command xsi:type="Post">
    <StartTime>16</StartTime>
    <Duration>5</Duration>
    <Message>... and the second stage has a single vacuum engine ...</Message>
  </Command>
  <Command xsi:type="Post">
    <StartTime>22</StartTime>
    <Duration>5</Duration>
    <Message>... the rocket is 82m x 7.1m, and the fairing is very shiny.</Message>
  </Command>
  <Command xsi:type="Post">
    <StartTime>28</StartTime>
    <Duration>5</Duration>
    <Message>Let's assume a T/W of 1.3, for a GLOW of 1,350 mT ...</Message>
  </Command>
  <Command xsi:type="Post">
    <StartTime>34</StartTime>
    <Duration>5</Duration>
    <Message>... including a 45mT payload, and see what it can do.</Message>
  </Command>
  <Command xsi:type="Post">
    <StartTime>40</StartTime>
    <Duration>5</Duration>
    <Message>Increasing update speed to 4x.</Message>
  </Command>
  <Command xsi:type="RelativePitch">
    <StartTime>50</StartTime>
    <Duration>50</Duration>
    <TargetOrientation>-144</TargetOrientation>
  </Command>
  <Command xsi:type="RelativePitch">
    <StartTime>100</StartTime>
    <Duration>80</Duration>
    <TargetOrientation>-158</TargetOrientation>
  </Command>
  <Command xsi:type="Post">
    <StartTime>167</StartTime>
    <Duration>5</Duration>
    <Message>New Glenn should stage at a much higher velocity than F9 ...</Message>
  </Command>
  <Command xsi:type="Post">
    <StartTime>173</StartTime>
    <Duration>5</Duration>
    <Message>... somewhere around 3,000 m/s.</Message>
  </Command>
  <Command xsi:type="Stage">
    <StartTime>180</StartTime>
  </Command>
  <Command xsi:type="RelativePitch">
    <StartTime>180</StartTime>
    <Duration>10</Duration>
    <TargetOrientation>-155</TargetOrientation>
  </Command>
  <Command xsi:type="RelativePitch">
    <StartTime>190</StartTime>
    <Duration>50</Duration>
    <TargetOrientation>-175</TargetOrientation>
  </Command>
  <Command xsi:type="Deploy">
    <Part>Fairing</Part>
    <StartTime>200</StartTime>
  </Command>
  <Command xsi:type="Post">
    <StartTime>206</StartTime>
    <Duration>5</Duration>
    <Message>The New Glenn S2 will have much better T/W than the current Falcon S2 ...</Message>
  </Command>
  <Command xsi:type="Post">
    <StartTime>212</StartTime>
    <Duration>5</Duration>
    <Message>... and should have a correspondingly shorter burn time ...</Message>
  </Command>
  <Command xsi:type="Post">
    <StartTime>218</StartTime>
    <Duration>5</Duration>
    <Message>... increasing update speed to 8x.</Message>
  </Command>
  <Command xsi:type="RelativePitch">
    <StartTime>240</StartTime>
    <Duration>60</Duration>
    <TargetOrientation>-180</TargetOrientation>
  </Command>
  <Command xsi:type="RelativePitch">
    <StartTime>300</StartTime>
    <Duration>60</Duration>
    <TargetOrientation>-185</TargetOrientation>
  </Command>
  <Command xsi:type="Post">
    <StartTime>381</StartTime>
    <Duration>5</Duration>
    <Message>... and reaching orbital velocity in around 400 seconds.</Message>
  </Command>
  <Command xsi:type="Post">
    <StartTime>420</StartTime>
    <Duration>5</Duration>
    <Message>Meanwhile the S1 ballistic trajectory is descending into the upper atmosphere ...</Message>
  </Command>
  <Command xsi:type="Post">
    <StartTime>426</StartTime>
    <Duration>5</Duration>
    <Message>... and the control surfaces are beginning to take effect.</Message>
  </Command>
  <Command xsi:type="Post">
    <StartTime>432</StartTime>
    <Duration>5</Duration>
    <Message>If the booster can generate sufficient lift ...</Message>
  </Command>
  <Command xsi:type="Post">
    <StartTime>438</StartTime>
    <Duration>5</Duration>
    <Message>... it could wash off velocity in the higher atmosphere ...</Message>
  </Command>
  <Command xsi:type="Post">
    <StartTime>444</StartTime>
    <Duration>5</Duration>
    <Message>... without excessive heating of the engine bells ...</Message>
  </Command>
  <Command xsi:type="Post">
    <StartTime>450</StartTime>
    <Duration>5</Duration>
    <Message>... aided by the novel shrouding.</Message>
  </Command>
  <Command xsi:type="Post">
    <StartTime>456</StartTime>
    <Duration>5</Duration>
    <Message>It would however glide a significant distance downrange.</Message>
  </Command>
  <Command xsi:type="Prograde">
    <StartTime>555</StartTime>
    <Duration>3000</Duration>
    <InitialAdjustmentTime>25</InitialAdjustmentTime>
  </Command>
  <Command xsi:type="Stage">
    <StartTime>1030</StartTime>
  </Command>
</ArrayOfCommand>