<?xml version="1.0"?>
<ArrayOfCommand xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Command xsi:type="RelativePitch">
    <StartTime>0</StartTime>
    <Duration>0</Duration>
    <TargetOrientation>-125.8</TargetOrientation>
  </Command>
  <Command xsi:type="RelativePitch">
    <StartTime>1</StartTime>
    <Duration>119</Duration>
    <TargetOrientation>-125.8</TargetOrientation>
  </Command>
  
  <!-- level out, limit Gs to 3 -->
  <Command xsi:type="RelativePitch">
    <StartTime>120</StartTime>
    <Duration>10</Duration>
    <TargetOrientation>-172</TargetOrientation>
  </Command>
  <Command xsi:type="RelativePitch">
    <StartTime>130</StartTime>
    <Duration>30</Duration>
    <TargetOrientation>-172</TargetOrientation>
  </Command>
  
  <!-- roll to 85, increase pitch -->
  <Command xsi:type="Roll">
    <StartTime>160</StartTime>
    <Duration>10</Duration>
    <TargetOrientation>85</TargetOrientation>
  </Command>
  <Command xsi:type="RelativePitch">
    <StartTime>170</StartTime>
    <Duration>10</Duration>
    <TargetOrientation>-142</TargetOrientation>
  </Command>
  <Command xsi:type="RelativePitch">
    <StartTime>180</StartTime>
    <Duration>120</Duration>
    <TargetOrientation>-125</TargetOrientation>
  </Command>
  
  <Command xsi:type="Roll">
    <StartTime>300</StartTime>
    <Duration>10</Duration>
    <TargetOrientation>-70</TargetOrientation>
  </Command>
  <Command xsi:type="RelativePitch">
    <StartTime>300</StartTime>
    <Duration>7</Duration>
    <TargetOrientation>-180</TargetOrientation>
  </Command>
  <Command xsi:type="RelativePitch">
    <StartTime>313</StartTime>
    <Duration>7</Duration>
    <TargetOrientation>-135</TargetOrientation>
  </Command>
  <Command xsi:type="RelativePitch">
    <StartTime>320</StartTime>
    <Duration>100</Duration>
    <TargetOrientation>-125</TargetOrientation>
  </Command>

  <Command xsi:type="Roll">
    <StartTime>430</StartTime>
    <Duration>10</Duration>
    <TargetOrientation>0</TargetOrientation>
  </Command>
  <Command xsi:type="RelativePitch">
    <StartTime>430</StartTime>
    <Duration>10</Duration>
    <TargetOrientation>-120</TargetOrientation>
  </Command>
  <Command xsi:type="RelativePitch">
    <StartTime>440</StartTime>
    <Duration>80</Duration>
    <TargetOrientation>-120</TargetOrientation>
  </Command>

  <Command xsi:type="Retrograde">
    <StartTime>540</StartTime>
    <Duration>80</Duration>
    <InitialAdjustmentTime>10</InitialAdjustmentTime>
  </Command>

  <Command xsi:type="Ignition">
    <StartTime>550</StartTime>
    <Duration>0.1</Duration>
    <Throttle>60</Throttle>
    <EngineIds>
      <int>6</int>
      <int>7</int>
      <int>8</int>
    </EngineIds>
  </Command>
  <Command xsi:type="Throttle">
    <StartTime>551</StartTime>
    <Duration>28</Duration>
    <TargetThrottle>40</TargetThrottle>
  </Command>
  <Command xsi:type="Shutdown">
    <StartTime>570</StartTime>
    <Duration>0.5</Duration>
  </Command>

  <Command xsi:type="AutoLand">
    <StartTime>590</StartTime>
    <Duration>30</Duration>
    <EngineIds>
      <int>6</int>
      <int>7</int>
      <int>8</int>
    </EngineIds>
  </Command>
</ArrayOfCommand>